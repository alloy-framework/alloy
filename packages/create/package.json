{
  "name": "@alloy-js/create",
  "version": "0.23.0",
  "description": "Create an Alloy project with npm init @alloy-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alloy-framework/alloy.git"
  },
  "main": "./dist/src/index.js",
  "bin": "./dist/src/index.js",
  "scripts": {
    "build": "node scripts/gen-deps.js && alloy build --with-dev",
    "clean": "rimraf dist/ .temp/",
    "test": "vitest run",
    "test:watch": "vitest -w",
    "watch": "alloy build --watch",
    "prepack": "node ../../scripts/strip-dev-exports.js"
  },
  "keywords": [],
  "author": "brian.terlson@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "@alloy-js/core": "workspace:~",
    "@alloy-js/typescript": "workspace:~",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@alloy-js/babel-plugin": "workspace:~",
    "@alloy-js/cli": "workspace:~",
    "@alloy-js/rollup-plugin": "workspace:~",
    "@rollup/plugin-typescript": "catalog:",
    "@types/node": "catalog:",
    "@types/prompts": "^2.4.9",
    "concurrently": "catalog:",
    "js-yaml": "^4.1.0",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json",
    "deps.json",
    "deps-versions.json"
  ]
}
